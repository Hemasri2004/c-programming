#include <stdio.h>  
#include <string.h>  
int main()  
{  
    char str[50];
    int i,lc=0,spl=0;
    scanf("%s",str);
    for(i=0;i<strlen(str);i++){
        if(str[i]>='a'&&str[i]<='z'){
            lc++;
        }
        else if(str[i]!=(str[i]>='A'&&str[i]<='Z')){
          spl++;
        }
    
    }
    printf("lower case=%d\nspl case=%d\n",lc,spl);
    return 0;  
}  
