//code-hema print biggest of three  numbers
#include <stdio.h>
void big(int a,int b, int c);
int main()
{
    int x,y,z;
    scanf("%d%d%d",&x,&y,&z);
     big(x,y,z);
    return 0;
}
void  big(int a,int b,int c)
{
   if(a>b && a>c)
   printf("%d",a);
   else if(b>a && b>c)
    printf("%d",b);
    else
    printf("%d",c);
}
